# Docker image registry (e.g. docker.io/username or ghcr.io/org)
IMAGE_REGISTRY=your-dockerhub-username
# Tag used when pulling images (usually latest or a specific release tag)
IMAGE_TAG=latest

# Plausible Analytics configuration
PLAUSIBLE_BASE_URL=https://dashboard.kunstradbasketball.de
PLAUSIBLE_SECRET_KEY_BASE=replace-with-64-byte-secret
PLAUSIBLE_DB_NAME=plausible
PLAUSIBLE_DB_USER=plausible
PLAUSIBLE_DB_PASSWORD=change-me
PLAUSIBLE_CLICKHOUSE_DB=plausible
# Set to "true" once der erste Account angelegt wurde
PLAUSIBLE_DISABLE_REGISTRATION=true
